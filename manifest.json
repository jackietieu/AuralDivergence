{
  "name": "AuralDivergence",
  "description" : "An extension for finding similar artists to the artist that is highlighted on the screen.",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "https://ajax.googleapis.com/",
    "http://api.musicgraph.com/*",
    "activeTab"
  ],
  "background": {
    "scripts": ["popup.js"]
  }
}
